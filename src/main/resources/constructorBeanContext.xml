<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="carSpecification" class="car.constructor.injection.Specification" >
        <property name="brand" value="TATA" />
        <property name="model" value="Tata Punch" />
    </bean>

    <bean id="myCar" class="car.constructor.injection.MyCar">
        <constructor-arg ref="carSpecification"/>
    </bean>

</beans>